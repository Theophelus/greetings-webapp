<div class="container">
    <div class="row">
        <h5 style="border-bottom: 1px solid; text-align:center">NUMBER OF USERS WHO HAVE BEEN GREETED</h5>

        <h6 style="text-align: center">DISPLAY the number of people who have been greeted</h6>

        <div class="display">
            <ul>
                {{#each user_name}}
                <li><a href="/counter/{{user_name}}">{{user_name}}</a> has been greeted.</li>
                {{!-- <li><a href="/counter/:users">{{@key}}</a> has been greeted.</li> --}}
                {{!-- @key is used to print every property inside an Object --}}
                {{/each}}
            </ul>
        </div>
    </div>
</div>
